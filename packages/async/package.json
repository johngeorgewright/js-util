{
  "name": "@johngw/async",
  "version": "5.0.1",
  "description": "JS/TS Async utilities.",
  "type": "module",
  "exports": {
    "./*": "./dist/*.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean && yarn tsc && rimraf --glob 'dist/**/?(__tests__|__mocks__|__setup__|*.test.*)'",
    "clean": "rimraf dist",
    "start": "yarn tsc --watch --preserveWatchOutput",
    "test": "vitest --run"
  },
  "author": "John Wright <johngeorge.wright@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/js-util/issues"
  },
  "homepage": "https://github.com/johngeorgewright/js-util#readme",
  "devDependencies": {
    "@types/node": "24.8.0",
    "rimraf": "6.0.1",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@johngw/object": "workspace:*",
    "tslib": "^2.8.1"
  }
}
