{
  "name": "@johngw/fs",
  "version": "3.0.1",
  "description": "JS/TS File System Utilities.",
  "main": "dist/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean && yarn tsc && rimraf --glob 'dist/**/?(__tests__|__mocks__|__setup__|*.test.*)'",
