{
  "name": "@johngw/fs",
  "version": "3.0.1",
  "description": "JS/TS File System Utilities.",
  "type": "module",
  "exports": {
    "./*": "./dist/*.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean && tsc && rimraf --glob 'dist/**/?(__tests__|__mocks__|__setup__|*.test.*)'",
    "clean": "rimraf dist",
    "start": "tsc --watch --preserveWatchOutput",
    "test": "vitest --run"
  },
  "author": "John Wright <johngeorge.wright@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/js-util/issues"
  },
  "homepage": "https://github.com/johngeorgewright/js-util#readme",
  "devDependencies": {
    "@johngw/async-iterator": "workspace:*",
    "@types/node": "24.8.0",
    "rimraf": "6.0.1",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
