{
  "compilerOptions": {
    "target": "es2022",
    "module": "nodenext",
    "lib": ["dom", "es2022"],
    "importHelpers": true,
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
